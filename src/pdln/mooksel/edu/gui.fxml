<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.Pane?>

<Pane style="or: BL;" styleClass="root" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pdln.mooksel.edu.GUIController">
   <children>
      <VBox layoutY="-2.0" spacing="10.0">
         <children>
            <Pane styleClass="display_wrap">
               <children>
                  <Canvas fx:id="display" height="480.0" layoutX="5.0" layoutY="5.0" styleClass="display" width="640.0" />
               </children>
               <opaqueInsets>
                  <Insets bottom="-5.0" left="-5.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="-5.0" left="-5.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Pane>
            <Pane fx:id="controlBar" layoutY="537.0" prefHeight="93.0" prefWidth="650.0">
               <children>
                  <Label fx:id="speedLabel" layoutX="472.0" layoutY="34.0" prefHeight="21.0" prefWidth="73.0" styleClass="root" />
                  <Slider fx:id="speedSlider" layoutX="173.0" layoutY="28.0" prefHeight="33.0" prefWidth="257.0" />
                  <Button fx:id="playButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onPlayButtonClock" prefHeight="60.0" prefWidth="100.0" styleClass="play_button" text="play" textAlignment="CENTER" wrapText="true" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Pane>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</Pane>
